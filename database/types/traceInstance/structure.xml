    <field foreignKey="nabu.providers.trace.persisted.database.types.traceService:id" name="traceServiceId" pattern="[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}|[0-9a-fA-F]{32}" type="java.util.UUID"/>
    <field name="started" timezone="UTC" type="java.util.Date"/>
    <field minOccurs="0" name="stopped" timezone="UTC" type="java.util.Date"/>
    <field minOccurs="0" name="alias" type="java.lang.String"/>
    <field minOccurs="0" name="authenticationId" type="java.lang.String"/>
    <field minOccurs="0" name="authenticator" type="java.lang.String"/>
    <field minOccurs="0" name="correlationId" type="java.lang.String"/>
    <field minOccurs="0" name="impersonator" type="java.lang.String"/>
    <field minOccurs="0" name="impersonatorId" type="java.lang.String"/>
    <field minOccurs="0" name="realm" type="java.lang.String"/>
    <field minOccurs="0" name="error" type="java.lang.String"/>
    <field minOccurs="0" name="serverName" type="java.lang.String"/>
    <field minOccurs="0" name="serverGroup" type="java.lang.String"/>
</structure>
